{
	"latex-workshop.latex.recipes": [
		{
			"name": "latexmk pft",
			"tools": ["latexmk"]
		}
	],
	"latex-workshop.latex.tools": [
		{
			"name": "latexmk",
			"command": "latexmk",
			"args": [
				"-f",
				"-bibtex",
				"-pdflatex",
				"-interaction=nonstopmode",
				"-shell-escape",
				"-synctex=1",
				"-file-line-error",
				"-pdf",
				"-outdir=%OUTDIR%",
				"%DOC%"
			],
			"env": {}
		}
	],
	"cSpell.words": [
		"pocketbase",
		"Signout",
		"Signup",
		"svgs",
		"tailwindcss",
		"Widgit"
	],
	"eslint.workingDirectories": [{ "mode": "auto" }]
}
